<%= render 'layouts/header_adm'%>
<br><br>
<h1 style="text-align: center;">Data UKT MTC</h1>
<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <% if notice.present?%>
        <p style="text-align: center" id="notice" class="alert alert-success"><%= notice %></p>
      <%end%>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-12">
		<%= link_to 'Edit', edit_geup_path(@geup), class: 'btn btn-warning float-right ml-2 mb-2' %> 
		<%= link_to 'Back', geups_path , class: 'btn btn-primary float-right mb-2'%>
    	<table class="table mx-auto">
    		<tr>
    			<td>Geup</td>
    			<td>:</td>
    			<td><%= @geup.geup %></td>
    		</tr>

    		<tr>
    			<td>Warna Sabuk</td>
    			<td>:</td>
    			<td><%= @geup.warna_string %></td>
    		</tr>

    	</table>
    </div>
</div>
</div>

